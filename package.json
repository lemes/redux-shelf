{
  "name": "redux-shelf",
  "version": "0.0.9",
  "main": "cjs/redux-shelf.js",
  "module": "es/redux-shelf.js",
  "browser": "dist/redux-shelf.js",
  "description": "Helper reducers and actions to take away your boilerplate code from both Data and Communication state types",
  "scripts": {
    "dev": "rollup -c -w",
    "build:cjs": "cross-env NODE_ENV=cjs rollup -c -o cjs/redux-shelf.js",
    "build:es": "cross-env BABEL_ENV=es NODE_ENV=es rollup -c -o es/redux-shelf.js",
    "build:umd": "cross-env BABEL_ENV=es NODE_ENV=development rollup -c -o dist/redux-shelf.js",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production rollup -c -o dist/redux-shelf.min.js",
    "build": "npm run build:cjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "test": "jest",
    "format": "prettier --write {src,test}/**/*.js",
    "precommit": "pretty-quick --staged",
    "test:coverage": "npm test -- --coverage",
    "eslint-check": "eslint --print-config .eslintrc | eslint-config-prettier-check"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lemes/redux-shelf.git"
  },
  "authors": [
    "Guilherme Popolin (https://github.com/guilhermespopolin)",
    "Rener Baffa (https://github.com/renerbaffa)",
    "Vin√≠cius Lemes (https://github.com/lemes)"
  ],
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@babel/core": "7.4.3",
    "@babel/plugin-transform-spread": "7.2.2",
    "@babel/preset-env": "7.4.3",
    "babel-jest": "24.7.1",
    "cross-env": "5.2.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jest": "22.4.1",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "husky": "1.3.1",
    "jest": "24.7.1",
    "normalizr": "3.3.0",
    "prettier": "1.17.0",
    "pretty-quick": "1.10.0",
    "rollup": "1.10.1",
    "rollup-plugin-analyzer": "3.0.0",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-cleaner": "0.2.0",
    "rollup-plugin-commonjs": "9.3.4",
    "rollup-plugin-node-resolve": "4.2.3",
    "rollup-plugin-uglify": "6.0.2"
  },
  "peerDependencies": {
    "normalizr": "3.3.0"
  },
  "dependencies": {
    "lodash.has": "4.5.2",
    "lodash.isstring": "4.0.1",
    "lodash.pick": "4.4.0"
  }
}
